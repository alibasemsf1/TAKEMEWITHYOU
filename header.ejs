        <link rel="stylesheet" href="/common_js_css/js/airdatepicker/datepicker.min.css">
        <link rel="stylesheet" href="/assets/admin/css/style.css">
        <link rel="icon" href="<% if(themeSettingJson.favicon){%>/assets/uploads/logo+icon/<%= themeSettingJson.favicon%><%}else{%>/assets/images/favicon.png<%}%>">
    </head>
    <body>
