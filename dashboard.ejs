                <div class="table-responsive">
                    <table id="example" class="table ad_table" style="width:100%">
                        <thead>
                            <tr>
                                <th>S.No.</th>
                                <th>Order Number</th>
                                <th>Order Date</th>
                                <th>Customer Name</th>
                                <th>Amount</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% let index=1; for(let i in order){%>
                                <tr>
                                    <td><%=index%></td>
                                    <td><%=order[i].order_number%></td>
                                    <td><%=order[i].order_date%></td>
                                    <td><%=order[i].customer_name%></td>
                                    <td><%=order[i].amount%></td>
                                </tr>
                            <% index++; } %> 
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<% include footer %>

<script src="assets/js/page_js/dashboard.js"></script>
<script type="text/javascript" src="/common_js_css/js/daterangepicker.min.js"></script>
<link rel="stylesheet" type="text/css" href="/common_js_css/css/daterangepicker.css" />

<script type="text/javascript">
    $(function() {
    
        var start = moment().subtract(6, 'days');
        var end = moment();
    
        function cb(start, end) {
            $('#reportrange span').html(start.format('MMMM D, YYYY') + ' - ' + end.format('MMMM D, YYYY'));
        }
    
        $('#reportrange').daterangepicker({
            startDate: start,
            endDate: end,
            ranges: {
            //    'Today': [moment(), moment()],
            //    'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],
               'This Week': [moment().startOf('week').toDate(), moment()],
               'Last 7 Days': [moment().subtract(6, 'days'), moment()],
               'Last 30 Days': [moment().subtract(29, 'days'), moment()],
               'This Month': [moment().startOf('month'), moment().endOf('month')],
               'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')]
            }
        }, cb);
    
        cb(start, end);
        
    
    });
    </script>
</body>
